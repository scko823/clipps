{
	"presets": [
		[
			"env",
			{
				"targets": {
					"browsers": ["last 3 versions", "safari >= 7"]
				}
			}
		],
		"react",
		"flow"
	],
	"plugins": [
		"transform-decorators-legacy",
		"transform-class-properties",
		"transform-object-rest-spread",
		"transform-react-remove-prop-types"
	]
}
